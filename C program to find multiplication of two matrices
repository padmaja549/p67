#include<stdio.h>
#include<conio.h>
void main()
{
	int a[3][2],b[3][2],c[3][2],i,j;
	clrscr();
	printf("Enter value for 1st matrix: ");
	for(i=0;i<3;i++)
	{
		for(j=0;j<2;j++)
		{
			scanf("%d",&a[i][j]);
		}
	}
	printf("Enter value for 2nd matrix: ");
	for(i=0;i<3;i++)
	{
		for(j=0;j<2;j++)
		{
			scanf("%d",&b[i][j]);
		}
	}
	for(i=0;i<3;i++)
	{
		for(j=0;j<2;j++)
		{
			c[i][j]=a[i][j]*b[i][j];
		}
	}
	printf("matrix isn");
	for(i=0;i<3;i++)
	{
	for(j=0;j<2;j++)
	{
		printf("%d ",c[i][j]);
	}
	printf("n");
	}
	getch();
}
